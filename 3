from collections import Counter
class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        count=Counter()
        left=0
        right=0
        res=0
        while right < len(s):
            r=s[right]
            count[r]+=1
            while count[r]>1:
                l=s[left]
                count[l]-=1
                left+=1
            res=max(res, right-left+1)
            right+=1
        return res
